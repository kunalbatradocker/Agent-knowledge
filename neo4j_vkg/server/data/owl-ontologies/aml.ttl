@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://purplefabric.ai/aml#> .

# Ontology Declaration
:AMLOntology a owl:Ontology ;
    rdfs:label "Enhanced Anti-Money Laundering Ontology" ;
    rdfs:comment "High-standard AML ontology with comprehensive compliance relationships and regulatory requirements" ;
    owl:versionInfo "2.0" .

# ============================================================================
# CLASSES
# ============================================================================

:SuspiciousActivity a owl:Class ;
    rdfs:label "Suspicious Activity" ;
    rdfs:comment "Suspicious activity requiring investigation" .

:RiskIndicator a owl:Class ;
    rdfs:label "Risk Indicator" ;
    rdfs:comment "Risk factor or red flag in transactions" .

:Investigation a owl:Class ;
    rdfs:label "Investigation" ;
    rdfs:comment "AML investigation case with findings" .

:SanctionedEntity a owl:Class ;
    rdfs:label "Sanctioned Entity" ;
    rdfs:comment "Entity on sanctions or watchlists" .

:PEP a owl:Class ;
    rdfs:label "PEP" ;
    rdfs:comment "Politically Exposed Person with enhanced due diligence" .

:Jurisdiction a owl:Class ;
    rdfs:label "Jurisdiction" ;
    rdfs:comment "Geographic jurisdiction with AML implications" .

:ComplianceRule a owl:Class ;
    rdfs:label "Compliance Rule" ;
    rdfs:comment "AML regulation or compliance requirement" .

:Alert a owl:Class ;
    rdfs:label "Alert" ;
    rdfs:comment "Automated AML system alert" .

:Report a owl:Class ;
    rdfs:label "Report" ;
    rdfs:comment "Regulatory report (SAR, CTR, FBAR)" .

:Watchlist a owl:Class ;
    rdfs:label "Watchlist" ;
    rdfs:comment "Sanctions or monitoring list" .

:RiskAssessment a owl:Class ;
    rdfs:label "Risk Assessment" ;
    rdfs:comment "Customer or transaction risk evaluation" .

:ComplianceOfficer a owl:Class ;
    rdfs:label "Compliance Officer" ;
    rdfs:comment "AML compliance staff member" .

:AuditTrail a owl:Class ;
    rdfs:label "Audit Trail" ;
    rdfs:comment "Record of compliance actions and decisions" .

:TrainingRecord a owl:Class ;
    rdfs:label "Training Record" ;
    rdfs:comment "AML training completion record" .

:PolicyDocument a owl:Class ;
    rdfs:label "Policy Document" ;
    rdfs:comment "AML policy or procedure document" .

:Typology a owl:Class ;
    rdfs:label "Typology" ;
    rdfs:comment "Money laundering method or pattern" .

# ============================================================================
# ENHANCED OBJECT PROPERTIES
# ============================================================================

# Suspicious Activity Relationships
:activityTriggeredBy a owl:ObjectProperty ;
    rdfs:label "Activity Triggered By" ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range :RiskIndicator .

:activityInvolvesCustomer a owl:ObjectProperty ;
    rdfs:label "Activity Involves Customer" ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range :Customer .

:activityInvolvesAccount a owl:ObjectProperty ;
    rdfs:label "Activity Involves Account" ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range :Account .

:activityInvolvesTransaction a owl:ObjectProperty ;
    rdfs:label "Activity Involves Transaction" ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range :Transaction .

:activityMatchesTypology a owl:ObjectProperty ;
    rdfs:label "Activity Matches Typology" ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range :Typology .

:activityInvestigatedBy a owl:ObjectProperty ;
    rdfs:label "Activity Investigated By" ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range :Investigation .

:activityReportedIn a owl:ObjectProperty ;
    rdfs:label "Activity Reported In" ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range :Report .

:activityRelatedTo a owl:ObjectProperty ;
    rdfs:label "Activity Related To" ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range :SuspiciousActivity .

# Investigation Relationships
:investigationAssignedTo a owl:ObjectProperty ;
    rdfs:label "Investigation Assigned To" ;
    rdfs:domain :Investigation ;
    rdfs:range :ComplianceOfficer .

:investigationCovers a owl:ObjectProperty ;
    rdfs:label "Investigation Covers" ;
    rdfs:domain :Investigation ;
    rdfs:range [ owl:unionOf ( :Customer :Account :Transaction :SuspiciousActivity ) ] .

:investigationResultsIn a owl:ObjectProperty ;
    rdfs:label "Investigation Results In" ;
    rdfs:domain :Investigation ;
    rdfs:range :Report .

:investigationFollowsUp a owl:ObjectProperty ;
