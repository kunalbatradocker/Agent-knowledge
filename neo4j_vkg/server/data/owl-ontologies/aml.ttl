# Suspicious Activity Properties
:activityId a owl:DatatypeProperty ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range xsd:string .

:activityType a owl:DatatypeProperty ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range [
        a rdfs:Datatype ;
        owl:oneOf ( "structuring" "unusual_pattern" "high_risk_jurisdiction" "sanctions_match" "pep_activity" "cash_intensive" "rapid_movement" "shell_company" )
    ] .

:detectionDate a owl:DatatypeProperty ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range xsd:dateTime .

:activityDescription a owl:DatatypeProperty ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range xsd:string .

:suspicionLevel a owl:DatatypeProperty ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range [
        a rdfs:Datatype ;
        owl:oneOf ( "low" "medium" "high" "critical" )
    ] .

:activityStatus a owl:DatatypeProperty ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range [
        a rdfs:Datatype ;
        owl:oneOf ( "detected" "under_review" "investigated" "reported" "closed" "false_positive" )
    ] .

:totalAmount a owl:DatatypeProperty ;
    rdfs:domain :SuspiciousActivity ;
    rdfs:range xsd:decimal .

:transactionCount a owl:DatatypeProperty ;
