    rdfs:range xsd:string .

:officerTitle a owl:DatatypeProperty ;
    rdfs:domain :ComplianceOfficer ;
    rdfs:range xsd:string .

:certifications a owl:DatatypeProperty ;
    rdfs:domain :ComplianceOfficer ;
    rdfs:range xsd:string .

:experienceYears a owl:DatatypeProperty ;
    rdfs:domain :ComplianceOfficer ;
    rdfs:range xsd:integer .

:specializations a owl:DatatypeProperty ;
    rdfs:domain :ComplianceOfficer ;
    rdfs:range xsd:string .

:caseloadCount a owl:DatatypeProperty ;
    rdfs:domain :ComplianceOfficer ;
    rdfs:range xsd:integer .

:lastTrainingDate a owl:DatatypeProperty ;
    rdfs:domain :ComplianceOfficer ;
    rdfs:range xsd:date .

:authorityLevel a owl:DatatypeProperty ;
    rdfs:domain :ComplianceOfficer ;
    rdfs:range [
        a rdfs:Datatype ;
        owl:oneOf ( "analyst" "senior_analyst" "manager" "senior_manager" "director" "chief_compliance_officer" )
    ] .
