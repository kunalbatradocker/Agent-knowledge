@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://example.org/resume#> .

# Ontology Declaration
:ResumeOntology a owl:Ontology ;
    rdfs:label "Enhanced Resume Ontology" ;
    rdfs:comment "High-standard ontology for comprehensive resume analysis with rich relationships and validation" ;
    owl:versionInfo "2.0" .

# ============================================================================
# CLASSES
# ============================================================================

:Candidate a owl:Class ;
    rdfs:label "Candidate" ;
    rdfs:comment "A person whose resume is being processed" .

:Skill a owl:Class ;
    rdfs:label "Skill" ;
    rdfs:comment "A technical or soft skill with proficiency levels" .

:Experience a owl:Class ;
    rdfs:label "Experience" ;
    rdfs:comment "Work experience with detailed context" .

:Company a owl:Class ;
    rdfs:label "Company" ;
    rdfs:comment "An organization or employer with industry context" .

:Education a owl:Class ;
    rdfs:label "Education" ;
    rdfs:comment "Educational qualification with academic details" .

:Institution a owl:Class ;
    rdfs:label "Institution" ;
    rdfs:comment "Educational institution with ranking and reputation" .

:Certification a owl:Class ;
    rdfs:label "Certification" ;
    rdfs:comment "Professional certification with validity tracking" .

:Project a owl:Class ;
    rdfs:label "Project" ;
    rdfs:comment "Personal or professional project with impact metrics" .

:Achievement a owl:Class ;
    rdfs:label "Achievement" ;
    rdfs:comment "Award or recognition with significance level" .

:Language a owl:Class ;
    rdfs:label "Language" ;
    rdfs:comment "Language skill with standardized proficiency levels" .

:Publication a owl:Class ;
    rdfs:label "Publication" ;
    rdfs:comment "Research publication with citation metrics" .

:Reference a owl:Class ;
    rdfs:label "Reference" ;
    rdfs:comment "Professional reference or recommendation" .

:Industry a owl:Class ;
    rdfs:label "Industry" ;
    rdfs:comment "Industry sector with specialization areas" .

:Role a owl:Class ;
    rdfs:label "Role" ;
    rdfs:comment "Job role or position type with seniority level" .

:Technology a owl:Class ;
    rdfs:label "Technology" ;
    rdfs:comment "Technology stack or tool category" .

# ============================================================================
# ENHANCED OBJECT PROPERTIES
# ============================================================================

# Skill Relationships
:hasSkill a owl:ObjectProperty ;
    rdfs:label "Has Skill" ;
    rdfs:domain :Candidate ;
    rdfs:range :Skill .

:skillUsedIn a owl:ObjectProperty ;
    rdfs:label "Skill Used In" ;
    rdfs:domain :Skill ;
    rdfs:range [ owl:unionOf ( :Experience :Project :Education ) ] .

:skillRequiredFor a owl:ObjectProperty ;
    rdfs:label "Skill Required For" ;
    rdfs:domain :Skill ;
    rdfs:range :Role .

:skillComplementedBy a owl:ObjectProperty ;
    rdfs:label "Skill Complemented By" ;
    rdfs:domain :Skill ;
    rdfs:range :Skill .

:skillPartOfTechnology a owl:ObjectProperty ;
    rdfs:label "Skill Part Of Technology" ;
    rdfs:domain :Skill ;
    rdfs:range :Technology .

# Experience Relationships
:hasExperience a owl:ObjectProperty ;
    rdfs:label "Has Experience" ;
    rdfs:domain :Candidate ;
    rdfs:range :Experience .

:experienceAtCompany a owl:ObjectProperty ;
    rdfs:label "Experience At Company" ;
    rdfs:domain :Experience ;
    rdfs:range :Company .

:experienceInRole a owl:ObjectProperty ;
