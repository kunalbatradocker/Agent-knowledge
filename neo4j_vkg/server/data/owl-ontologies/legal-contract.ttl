    rdfs:range :Party .

:signatureWitnessedBy a owl:ObjectProperty ;
    rdfs:label "Signature Witnessed By" ;
    rdfs:domain :Signature ;
    rdfs:range :Individual .

:signatureNotarizedBy a owl:ObjectProperty ;
    rdfs:label "Signature Notarized By" ;
    rdfs:domain :Signature ;
    rdfs:range :Individual .

