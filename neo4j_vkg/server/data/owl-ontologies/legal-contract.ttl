@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://purplefabric.ai/legal-contract#> .

# Ontology Declaration
:LegalContractOntology a owl:Ontology ;
    rdfs:label "Enhanced Legal Contract Ontology" ;
    rdfs:comment "High-standard legal contract ontology with comprehensive relationships and legal concepts" ;
    owl:versionInfo "2.0" .

# ============================================================================
# CLASSES
# ============================================================================

:Contract a owl:Class ;
    rdfs:label "Contract" ;
    rdfs:comment "Legal agreement between parties" .

:Party a owl:Class ;
    rdfs:label "Party" ;
    rdfs:comment "Legal entity party to the contract" .

:Clause a owl:Class ;
    rdfs:label "Clause" ;
    rdfs:comment "Specific provision in the contract" .

:Obligation a owl:Class ;
    rdfs:label "Obligation" ;
    rdfs:comment "Legal duty imposed by the contract" .

:Right a owl:Class ;
    rdfs:label "Right" ;
    rdfs:comment "Legal entitlement granted by the contract" .

:Term a owl:Class ;
    rdfs:label "Term" ;
    rdfs:comment "Defined term or condition" .

:Payment a owl:Class ;
    rdfs:label "Payment" ;
    rdfs:comment "Financial obligation or payment term" .

:Deliverable a owl:Class ;
    rdfs:label "Deliverable" ;
    rdfs:comment "Work product or service to be delivered" .

:Milestone a owl:Class ;
    rdfs:label "Milestone" ;
    rdfs:comment "Key project milestone or deadline" .

:Warranty a owl:Class ;
    rdfs:label "Warranty" ;
    rdfs:comment "Guarantee or assurance provided" .

:Indemnity a owl:Class ;
    rdfs:label "Indemnity" ;
    rdfs:comment "Protection against loss or damage" .

:Penalty a owl:Class ;
    rdfs:label "Penalty" ;
    rdfs:comment "Consequence for breach or non-performance" .

:Termination a owl:Class ;
    rdfs:label "Termination" ;
    rdfs:comment "Contract termination provision" .

:Dispute a owl:Class ;
    rdfs:label "Dispute" ;
    rdfs:comment "Disagreement or conflict resolution mechanism" .

:Jurisdiction a owl:Class ;
    rdfs:label "Jurisdiction" ;
    rdfs:comment "Legal jurisdiction governing the contract" .

:LegalEntity a owl:Class ;
    rdfs:label "Legal Entity" ;
    rdfs:comment "Corporation, LLC, or other legal entity" .

:Individual a owl:Class ;
    rdfs:label "Individual" ;
    rdfs:comment "Natural person party to contract" .

:Amendment a owl:Class ;
    rdfs:label "Amendment" ;
    rdfs:comment "Modification to the original contract" .

:Exhibit a owl:Class ;
    rdfs:label "Exhibit" ;
    rdfs:comment "Attachment or appendix to the contract" .

:Signature a owl:Class ;
    rdfs:label "Signature" ;
    rdfs:comment "Legal signature and execution details" .

# ============================================================================
# ENHANCED OBJECT PROPERTIES
# ============================================================================

# Contract Structure Relationships
:contractBetween a owl:ObjectProperty ;
    rdfs:label "Contract Between" ;
    rdfs:domain :Contract ;
    rdfs:range :Party .

:contractContains a owl:ObjectProperty ;
    rdfs:label "Contract Contains" ;
    rdfs:domain :Contract ;
    rdfs:range [ owl:unionOf ( :Clause :Term :Exhibit ) ] .

:contractGoverned a owl:ObjectProperty ;
    rdfs:label "Contract Governed" ;
    rdfs:domain :Contract ;
    rdfs:range :Jurisdiction .

:contractAmendedBy a owl:ObjectProperty ;
    rdfs:label "Contract Amended By" ;
    rdfs:domain :Contract ;
    rdfs:range :Amendment .

:contractSignedBy a owl:ObjectProperty ;
    rdfs:label "Contract Signed By" ;
    rdfs:domain :Contract ;
    rdfs:range :Signature .

:contractSupersedes a owl:ObjectProperty ;
    rdfs:label "Contract Supersedes" ;
    rdfs:domain :Contract ;
    rdfs:range :Contract .

:contractRelatedTo a owl:ObjectProperty ;
    rdfs:label "Contract Related To" ;
    rdfs:domain :Contract ;
    rdfs:range :Contract .

# Party Relationships
:partyHasObligation a owl:ObjectProperty ;
    rdfs:label "Party Has Obligation" ;
    rdfs:domain :Party ;
    rdfs:range :Obligation .

:partyHasRight a owl:ObjectProperty ;
    rdfs:label "Party Has Right" ;
    rdfs:domain :Party ;
    rdfs:range :Right .

:partyMakesPayment a owl:ObjectProperty ;
    rdfs:label "Party Makes Payment" ;
    rdfs:domain :Party ;
    rdfs:range :Payment .

:partyReceivesPayment a owl:ObjectProperty ;
    rdfs:label "Party Receives Payment" ;
    rdfs:domain :Party ;
    rdfs:range :Payment .

:partyDeliversTo a owl:ObjectProperty ;
    rdfs:label "Party Delivers To" ;
    rdfs:domain :Party ;
    rdfs:range :Deliverable .

:partyProvidesWarranty a owl:ObjectProperty ;
    rdfs:label "Party Provides Warranty" ;
    rdfs:domain :Party ;
    rdfs:range :Warranty .

:partyIndemnifies a owl:ObjectProperty ;
    rdfs:label "Party Indemnifies" ;
    rdfs:domain :Party ;
    rdfs:range :Party .

:partyRepresentedBy a owl:ObjectProperty ;
    rdfs:label "Party Represented By" ;
    rdfs:domain :Party ;
    rdfs:range :Individual .

:partySubsidiaryOf a owl:ObjectProperty ;
    rdfs:label "Party Subsidiary Of" ;
    rdfs:domain :Party ;
    rdfs:range :LegalEntity .

# Clause Relationships
:clauseImposes a owl:ObjectProperty ;
    rdfs:label "Clause Imposes" ;
    rdfs:domain :Clause ;
    rdfs:range :Obligation .

:clauseGrants a owl:ObjectProperty ;
    rdfs:label "Clause Grants" ;
    rdfs:domain :Clause ;
    rdfs:range :Right .

:clauseDefines a owl:ObjectProperty ;
    rdfs:label "Clause Defines" ;
    rdfs:domain :Clause ;
    rdfs:range :Term .

:clauseReferences a owl:ObjectProperty ;
    rdfs:label "Clause References" ;
    rdfs:domain :Clause ;
    rdfs:range [ owl:unionOf ( :Clause :Term :Exhibit ) ] .

:clauseModifiedBy a owl:ObjectProperty ;
    rdfs:label "Clause Modified By" ;
    rdfs:domain :Clause ;
    rdfs:range :Amendment .

:clauseContainsCondition a owl:ObjectProperty ;
    rdfs:label "Clause Contains Condition" ;
    rdfs:domain :Clause ;
    rdfs:range :Term .

:clauseSpecifiesPenalty a owl:ObjectProperty ;
    rdfs:label "Clause Specifies Penalty" ;
    rdfs:domain :Clause ;
    rdfs:range :Penalty .

# Obligation and Rights Relationships
:obligationTriggeredBy a owl:ObjectProperty ;
    rdfs:label "Obligation Triggered By" ;
    rdfs:domain :Obligation ;
    rdfs:range [ owl:unionOf ( :Milestone :Term :Payment ) ] .

:obligationDependsOn a owl:ObjectProperty ;
    rdfs:label "Obligation Depends On" ;
    rdfs:domain :Obligation ;
    rdfs:range :Obligation .

:rightConditionalOn a owl:ObjectProperty ;
    rdfs:label "Right Conditional On" ;
    rdfs:domain :Right ;
    rdfs:range [ owl:unionOf ( :Obligation :Term :Payment ) ] .

:rightLimitedBy a owl:ObjectProperty ;
    rdfs:label "Right Limited By" ;
    rdfs:domain :Right ;
    rdfs:range :Clause .

:obligationSecuredBy a owl:ObjectProperty ;
    rdfs:label "Obligation Secured By" ;
    rdfs:domain :Obligation ;
    rdfs:range :Warranty .

# Payment and Deliverable Relationships
:paymentFor a owl:ObjectProperty ;
    rdfs:label "Payment For" ;
    rdfs:domain :Payment ;
    rdfs:range :Deliverable .

:paymentTriggeredBy a owl:ObjectProperty ;
    rdfs:label "Payment Triggered By" ;
    rdfs:domain :Payment ;
    rdfs:range :Milestone .

:paymentSubjectTo a owl:ObjectProperty ;
    rdfs:label "Payment Subject To" ;
    rdfs:domain :Payment ;
    rdfs:range [ owl:unionOf ( :Term :Penalty ) ] .

:deliverableRequires a owl:ObjectProperty ;
    rdfs:label "Deliverable Requires" ;
    rdfs:domain :Deliverable ;
    rdfs:range :Deliverable .

:deliverableAcceptedBy a owl:ObjectProperty ;
    rdfs:label "Deliverable Accepted By" ;
    rdfs:domain :Deliverable ;
    rdfs:range :Party .

:milestoneLeadsTo a owl:ObjectProperty ;
    rdfs:label "Milestone Leads To" ;
    rdfs:domain :Milestone ;
    rdfs:range :Milestone .

# Warranty and Indemnity Relationships
:warrantyCovers a owl:ObjectProperty ;
    rdfs:label "Warranty Covers" ;
    rdfs:domain :Warranty ;
    rdfs:range :Deliverable .

:warrantyLimitedBy a owl:ObjectProperty ;
    rdfs:label "Warranty Limited By" ;
    rdfs:domain :Warranty ;
    rdfs:range :Term .

:indemnityCovers a owl:ObjectProperty ;
    rdfs:label "Indemnity Covers" ;
    rdfs:domain :Indemnity ;
    rdfs:range [ owl:unionOf ( :Obligation :Right :Deliverable ) ] .

:indemnityExcludes a owl:ObjectProperty ;
    rdfs:label "Indemnity Excludes" ;
    rdfs:domain :Indemnity ;
    rdfs:range :Term .

# Termination and Dispute Relationships
:terminationTriggeredBy a owl:ObjectProperty ;
    rdfs:label "Termination Triggered By" ;
    rdfs:domain :Termination ;
    rdfs:range [ owl:unionOf ( :Penalty :Dispute :Term ) ] .

:terminationAffects a owl:ObjectProperty ;
    rdfs:label "Termination Affects" ;
    rdfs:domain :Termination ;
    rdfs:range [ owl:unionOf ( :Obligation :Right :Payment ) ] .

:disputeResolvedBy a owl:ObjectProperty ;
    rdfs:label "Dispute Resolved By" ;
    rdfs:domain :Dispute ;
    rdfs:range :Jurisdiction .

:disputeInvolves a owl:ObjectProperty ;
    rdfs:label "Dispute Involves" ;
    rdfs:domain :Dispute ;
    rdfs:range :Party .

:disputeRegarding a owl:ObjectProperty ;
    rdfs:label "Dispute Regarding" ;
    rdfs:domain :Dispute ;
    rdfs:range [ owl:unionOf ( :Clause :Obligation :Payment ) ] .

# Amendment and Exhibit Relationships
:amendmentModifies a owl:ObjectProperty ;
    rdfs:label "Amendment Modifies" ;
