:exhibitIncorporatedBy a owl:ObjectProperty ;
    rdfs:label "Exhibit Incorporated By" ;
    rdfs:domain :Exhibit ;
    rdfs:range :Clause .

# Signature Relationships
:signatureBy a owl:ObjectProperty ;
    rdfs:label "Signature By" ;
